<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginadmin</servlet-name>
        <servlet-class>loginadmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createAcc</servlet-name>
        <servlet-class>createAcc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>depositeBal</servlet-name>
        <servlet-class>checkCustomer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>depositAmt</servlet-name>
        <servlet-class>depositAmt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>withdeawAmt</servlet-name>
        <servlet-class>withdrawAmt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>withdrawAmt</servlet-name>
        <servlet-class>withdrawAmt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userInfo</servlet-name>
        <servlet-class>userInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateUserInfo</servlet-name>
        <servlet-class>updateUserInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteAccount</servlet-name>
        <servlet-class>deleteAccount</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginadmin</servlet-name>
        <url-pattern>/loginadmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createAcc</servlet-name>
        <url-pattern>/createAcc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>depositeBal</servlet-name>
        <url-pattern>/depositeBal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>depositAmt</servlet-name>
        <url-pattern>/depositAmt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>withdeawAmt</servlet-name>
        <url-pattern>/withdeawAmt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>withdrawAmt</servlet-name>
        <url-pattern>/withdrawAmt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userInfo</servlet-name>
        <url-pattern>/userInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateUserInfo</servlet-name>
        <url-pattern>/updateUserInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteAccount</servlet-name>
        <url-pattern>/deleteAccount</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <display-name>home</display-name>
            <url-pattern>/admin/home.jsp</url-pattern>
        </jsp-property-group>
        <jsp-property-group>
            <display-name>createAcc</display-name>
            <url-pattern>/admin/createAcc.jsp</url-pattern>
        </jsp-property-group>
    </jsp-config>
</web-app>
